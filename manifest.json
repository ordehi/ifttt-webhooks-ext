{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A Chrome extension to interact with IFTTT Webhooks",
  "permissions": ["webRequest", "activeTab"],
  "host_permissions": ["https://maker.ifttt.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
